# Canonical host/native development template (copy to .env)
# Private keys are placeholders. Do not commit populated .env files.

NODE_ENV=development
API_PORT=3001
DATABASE_URL=postgres://postgres:postgres@localhost:5432/ticketdb

# Auth
SESSION_SECRET=CHANGE_ME_SESSION_SECRET
JWT_SECRET=CHANGE_ME_JWT_SECRET
JWT_EXPIRES_IN=7d

# OAuth
# Must be a Google Web OAuth client ID:
# <numeric-project-id>-<client>.apps.googleusercontent.com
GOOGLE_CLIENT_ID=CHANGE_ME_GOOGLE_CLIENT_ID
GOOGLE_CLIENT_SECRET=CHANGE_ME_GOOGLE_CLIENT_SECRET
GOOGLE_CALLBACK_URL=http://localhost:3001/api/auth/google/callback

# Client access
CLIENT_ORIGIN=http://localhost:5173
CORS_ORIGINS=http://localhost:5173

# Optional alerts/observability
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
ENABLE_LOGSTASH=false
LOGSTASH_HOST=logstash
LOGSTASH_PORT=5001
ELASTIC_URL=http://localhost:9200
