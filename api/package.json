{
  "name": "ecotrack-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build --workspace=ecotrack-database && npm run build && concurrently --kill-others-on-fail -n api,tsc \"cross-env NODE_ENV=development node dist/main.js\" \"tsc -p tsconfig.build.json -w\"",
    "build": "tsc -p tsconfig.build.json",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "start:debug": "cross-env NODE_ENV=development tsx src/main.ts",
    "lint": "eslint . --ext ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.3.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.45.1",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "ecotrack-database": "0.1.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "@eslint/js": "^9.14.0",
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-security": "^3.0.1",
    "supertest": "^7.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
