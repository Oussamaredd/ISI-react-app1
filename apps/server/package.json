{
  "name": "server",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "start:local": "cross-env ENV_FILE=.env.local node src/index.js",
    "start:docker": "cross-env ENV_FILE=.env node src/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/api.test.js --runInBand --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/api.test.js --watch",
    "migrate": "knex --knexfile src/config/knexfile.js migrate:latest",
    "migrate:rollback": "knex --knexfile src/config/knexfile.js migrate:rollback",
    "seed": "knex --knexfile src/config/knexfile.js seed:run"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "knex": "^3.1.0",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-elasticsearch": "^0.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "cross-env": "^10.1.0",
    "csurf": "^1.11.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "xss": "^1.0.15"
  }
}
