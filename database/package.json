{
  "name": "react-app1-database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "drizzle.config.ts",
    "migrations"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "watch": "tsc -w -p tsconfig.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "npm run build && node dist/seed.js",
    "db:migrate:seed": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "drizzle-kit": "^0.31.9",
    "typescript": "^5.7.2"
  }
}
