# Staging Environment Configuration
NODE_ENV=staging
PORT=3000

# Database - Staging PostgreSQL
DATABASE_URL=postgresql://username:password@your-staging-host:5432/ticketapp_staging
DATABASE_POOL_MIN=3
DATABASE_POOL_MAX=15

# Redis - Staging Redis
REDIS_URL=redis://username:password@your-staging-redis:6379

# Google OAuth - Staging credentials
GOOGLE_CLIENT_ID=38782580532-6oja0hr6gfulaqsdpkq8qf50qccipmgj.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=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

# Session and JWT secrets
SESSION_SECRET=de2d141070facd04408667f34aca8a20f2579dbe8de9217e7269275768ef0e8c
JWT_SECRET=ddc3632b9f7c7ad279963d82de48b73e0cdb6c7e10943c56508c879229de4b02
JWT_EXPIRES_IN=7d

# Application URLs
APP_URL=https://staging.yourdomain.com
API_URL=https://staging.yourdomain.com/api

# CORS origins (comma-separated)
CORS_ORIGINS=https://staging.yourdomain.com

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200

# Security
BCRYPT_ROUNDS=12
SESSION_MAX_AGE=604800000
SESSION_SECURE=true

# File upload
MAX_FILE_SIZE=5242880
UPLOAD_PATH=/var/www/uploads-staging

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Cache
CACHE_TTL=450
CACHE_MAX_SIZE=2500

# Features
ENABLE_REGISTRATION=true
ENABLE_EMAIL_VERIFICATION=true
ENABLE_2FA=false
ENABLE_SEED_DATA=false
ENABLE_SWAGGER=true

# External services
WEBHOOK_URL=https://staging.yourdomain.com/webhooks
WEBHOOK_SECRET=your-staging-webhook-secret-32-chars-minimum