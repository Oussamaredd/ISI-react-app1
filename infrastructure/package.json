{
  "name": "react-app1-infrastructure",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docker:dev": "docker compose -f docker-compose.yml --profile core up --build -d",
    "docker:down": "docker compose -f docker-compose.yml --profile core down --remove-orphans",
    "health": "pwsh ./health-check.bat",
    "migrate:up": "sh ./scripts/migrate.sh up",
    "migrate:status": "sh ./scripts/migrate.sh status"
  }
}
