{
  "dashboard": {
    "id": null,
    "title": "EcoTrack Metrics Overview",
    "tags": ["ecotrack", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "hideControls": false,
    "links": [],
    "refresh": "30s",
    "rows": [
      {
        "collapse": false,
        "height": "250px",
        "panels": [
          {
            "title": "Frontend Errors Ingested (rate)",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(frontend_errors_total[5m])",
                "legendFormat": "errors/sec"
              }
            ],
            "gridPos": {
              "h": 8,
              "w": 12,
              "x": 0,
              "y": 0
            },
            "id": 1
          },
          {
            "title": "Frontend Metrics Ingested (rate)",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(frontend_metrics_total[5m])",
                "legendFormat": "metrics/sec"
              }
            ],
            "gridPos": {
              "h": 8,
              "w": 12,
              "x": 12,
              "y": 0
            },
            "id": 2
          }
        ],
        "title": "Ingestion Rates"
      },
      {
        "collapse": false,
        "height": "250px",
        "panels": [
          {
            "title": "Frontend Error Buffer Size",
            "type": "graph",
            "targets": [
              {
                "expr": "frontend_error_buffer_size",
                "legendFormat": "error buffer"
              }
            ],
            "gridPos": {
              "h": 8,
              "w": 12,
              "x": 0,
              "y": 8
            },
            "id": 3
          },
          {
            "title": "Frontend Metric Buffer Size",
            "type": "graph",
            "targets": [
              {
                "expr": "frontend_metric_buffer_size",
                "legendFormat": "metric buffer"
              }
            ],
            "gridPos": {
              "h": 8,
              "w": 12,
              "x": 12,
              "y": 8
            },
            "id": 4
          }
        ],
        "title": "Buffer Health"
      },
      {
        "collapse": false,
        "height": "250px",
        "panels": [
          {
            "title": "Errors by Type",
            "type": "graph",
            "targets": [
              {
                "expr": "frontend_errors_by_type_total",
                "legendFormat": "{{type}}"
              }
            ],
            "gridPos": {
              "h": 8,
              "w": 12,
              "x": 0,
              "y": 16
            },
            "id": 5
          },
          {
            "title": "Metrics by Type",
            "type": "graph",
            "targets": [
              {
                "expr": "frontend_metrics_by_type_total",
                "legendFormat": "{{type}}"
              }
            ],
            "gridPos": {
              "h": 8,
              "w": 12,
              "x": 12,
              "y": 16
            },
            "id": 6
          }
        ],
        "title": "Metric Distribution"
      }
    ],
    "schemaVersion": 14,
    "version": 1,
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": []
    }
  }
}
